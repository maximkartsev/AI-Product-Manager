APP_NAME=Boilerplate
APP_ENV=local
APP_KEY=base64:
APP_DEBUG=true
APP_URL=http://localhost

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

# PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=mysql
DB_HOST=mariadb
DB_PORT=3306
DB_DATABASE=bp
DB_USERNAME=root
DB_PASSWORD=root

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=s3
QUEUE_CONNECTION=database

CACHE_STORE=database
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

# MinIO endpoint reachable by the browser (ngrok recommended).
# For AWS deploys, use the CloudFront domain for public URLs:
# AWS_URL=https://<CloudFrontDomain>
AWS_ENDPOINT=https://minio.ngrok.pizza
AWS_URL=https://minio.ngrok.pizza/bp-media
AWS_ACCESS_KEY_ID=laradock
AWS_SECRET_ACCESS_KEY=laradock
AWS_DEFAULT_REGION=us-east-1
# Local dev MinIO bucket (create this in MinIO console).
AWS_BUCKET=bp-media
AWS_USE_PATH_STYLE_ENDPOINT=true

GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=

TIKTOK_CLIENT_ID=
TIKTOK_CLIENT_SECRET=

APPLE_CLIENT_ID=
APPLE_KEY_ID=
APPLE_TEAM_ID=
APPLE_PRIVATE_KEY=/var/www/storage/keys/Apple_AuthKey.p8

FRONTEND_URL=https://uitest.ngrok.app

VITE_APP_NAME="${APP_NAME}"
# MinIO endpoint reachable by the browser (ngrok recommended).
# For AWS deploys, use the CloudFront domain for public URLs:
# AWS_URL=https://<CloudFrontDomain>
AWS_ENDPOINT=https://minio.ngrok.pizza
AWS_URL=https://minio.ngrok.pizza/bp-media
AWS_ACCESS_KEY_ID=laradock
AWS_SECRET_ACCESS_KEY=laradock
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=bp-media
AWS_USE_PATH_STYLE_ENDPOINT=true

COMFYUI_DEFAULT_PROVIDER=cloud
COMFYUI_AWS_REGION=us-east-1
# When AWS_* points to MinIO, fill COMFYUI_* with real AWS S3 creds/buckets.
COMFYUI_MODELS_BUCKET=
COMFYUI_LOGS_BUCKET=
COMFYUI_MODELS_DISK=comfyui_models
COMFYUI_LOGS_DISK=comfyui_logs
COMFYUI_ASSET_OPS_SECRET=
COMFYUI_ASSET_UPLOAD_PREFIX=assets
COMFYUI_ASSET_BUNDLE_PREFIX=bundles
# Presigned URL TTLs (seconds). Multipart needs a longer window for large files.
COMFYUI_PRESIGNED_TTL_SECONDS=900
COMFYUI_MULTIPART_PRESIGNED_TTL_SECONDS=3600
COMFYUI_EMIT_WORKFLOW_METRICS=true
COMFYUI_EMIT_FLEET_METRICS=true

# ComfyUI Models/Logs (AWS S3-specific if different from main AWS_* block)
# Local dev example: media uses AWS_* (MinIO), models/logs use COMFYUI_* (AWS S3).
# Example (mixed: MinIO media, AWS S3 models/logs):
# COMFYUI_MODELS_BUCKET=bp-models-<account>-staging
# COMFYUI_LOGS_BUCKET=bp-logs-<account>-staging
# COMFYUI_MODELS_ACCESS_KEY_ID=<aws_access_key_id>
# COMFYUI_MODELS_SECRET_ACCESS_KEY=<aws_secret_access_key>
# COMFYUI_LOGS_ACCESS_KEY_ID=<aws_access_key_id>
# COMFYUI_LOGS_SECRET_ACCESS_KEY=<aws_secret_access_key>
# COMFYUI_MODELS_REGION=us-east-1
# COMFYUI_LOGS_REGION=us-east-1
# COMFYUI_MODELS_ENDPOINT=
# COMFYUI_LOGS_ENDPOINT=
# COMFYUI_MODELS_URL=
# COMFYUI_LOGS_URL=
# Leave COMFYUI_*_ENDPOINT empty to use real AWS S3 endpoints.
COMFYUI_MODELS_ACCESS_KEY_ID=
COMFYUI_MODELS_SECRET_ACCESS_KEY=
COMFYUI_MODELS_REGION=us-east-1
COMFYUI_MODELS_ENDPOINT=
COMFYUI_MODELS_URL=
COMFYUI_MODELS_USE_PATH_STYLE_ENDPOINT=false

COMFYUI_LOGS_ACCESS_KEY_ID=
COMFYUI_LOGS_SECRET_ACCESS_KEY=
COMFYUI_LOGS_REGION=us-east-1
COMFYUI_LOGS_ENDPOINT=
COMFYUI_LOGS_URL=
COMFYUI_LOGS_USE_PATH_STYLE_ENDPOINT=false

COMFYUI_FLEET_SECRET_STAGING=
COMFYUI_FLEET_SECRET_PRODUCTION=
